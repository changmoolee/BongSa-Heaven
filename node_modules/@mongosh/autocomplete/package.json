{
  "name": "@mongosh/autocomplete",
  "version": "1.1.5",
  "description": "MongoDB Shell Autocomplete Package",
  "homepage": "https://github.com/mongodb-js/mongosh",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/mongosh"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "scripts": {
    "test": "mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register \"./{src,lib}/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "lint": "eslint --report-unused-disable-directives \"./{src,test}/**/*.{js,ts,tsx}\"",
    "check": "npm run lint && depcheck --skip-missing=true",
    "compile-ts": "tsc -p tsconfig.json",
    "prepublish": "npm run compile-ts"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "devDependencies": {
    "mocha": "^7.1.2"
  },
  "dependencies": {
    "@mongosh/shell-api": "1.1.5",
    "mongodb-ace-autocompleter": "^0.8.0",
    "semver": "^7.3.2"
  },
  "gitHead": "d9cc9e28dc4ec29d11f612b8552aa3b093a1198b"
}
